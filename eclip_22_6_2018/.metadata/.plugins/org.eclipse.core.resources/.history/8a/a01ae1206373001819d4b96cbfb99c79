package com.bai24;

public class ThuVienChuoi {

	
	public static void main(String[] args) {
		StringBuilder sb = new StringBuilder();
		sb.append("Hello");
		sb.append("\n");
		sb.append("putin");
		sb.append("\n");
		sb.append("opama");
		sb.insert(5, "xxx")
		String s = sb.toString();
		System.out.println(s);
	}
}
