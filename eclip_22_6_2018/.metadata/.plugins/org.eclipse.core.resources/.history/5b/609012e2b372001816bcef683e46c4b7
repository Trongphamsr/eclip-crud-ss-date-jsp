public class TestXuathuoi {

	public static void main(String[] args) {
//		 xuat thu vien thuoc java.lang
//		 xuat tren 1 dong va khac dong
//		cac ky tu dac biet
//		\n xuong dong \t thut dau dong \\ trich dan
		System.out.println("me gia cho con ngoi dem la la roi");
		System.out.println("me gia cho con ngoi\n\tdem la la roi");
		System.out.println("me gia cho con ngoi\"dem\" la la roi");
		System.out.println("me gia cho con ngoi dem la la roi");
		System.out.println("xin chao cac ban");
		System.out.println("xin chao viet nam");
	}
}
