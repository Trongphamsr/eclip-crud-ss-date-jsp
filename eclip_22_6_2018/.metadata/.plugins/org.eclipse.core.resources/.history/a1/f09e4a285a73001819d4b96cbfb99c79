package com.bai24;

public class ThuVienToanHoc {

	public static void main(String[] args) {
		System.out.println("PI= "+Math.PI);
		int sonn =Math.min(9, 15);
		System.out.println("so min= "+sonn);
		double can = Math.sqrt(25);
		System.out.println("can bac hai= "+can);
		double gttd=Math.abs(-4);
		System.out.println("gttd= "+gttd);
		double lt = Math.pow(3, 4);
		System.out.println("3^4= "+lt);
	}
}
