package com.bai24;

public class XoaKhoangTrangDuThuThuatraiphaiTrim {

	public static void main(String[] args) {
		String s= " thay pham van trong ";
		System.out.println(s);
		System.out.println("chieu dai= "+s.length());
		s=s.trim();
		System.out.println(s);
		System.out.println("chieu dai= "+s.length());
	}
}
