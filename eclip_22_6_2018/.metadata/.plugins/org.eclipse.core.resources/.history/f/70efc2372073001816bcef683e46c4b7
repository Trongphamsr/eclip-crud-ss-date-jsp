package communityutil.com;

import java.util.Scanner;

public class For {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("ban hay nhap so nguyen");
		int a = sc.nextInt();
		int gt=1;
		for (int i = 1; ++i < a; i++) {
			gt=gt*i;
		}
		System.out.println(gt);
	}
}
