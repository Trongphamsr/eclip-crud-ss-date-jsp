package com.bai48;

public class HanCheMang {

	// kho xoa, kho mo rong...
	public static void main(String[] args) {
	
		int []M= {1,2,3,4,5};
		System.out.println(M[4]);
		int []M2=new int[M.length+1];
		for (int i = 0; i < M.length; i++) 
			M2[i]=M[i];
			M2[M2.length-1]=1;
		
			for (int i = 0; i < M2.length; i++) {
				System.out.print(M2[i]+"\t");
			}
	}
}
