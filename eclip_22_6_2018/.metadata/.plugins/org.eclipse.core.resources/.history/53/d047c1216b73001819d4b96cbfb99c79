package com.bai24;

public class HamTimChuoi {
	public static void main(String[] args) {
		String s = "Hello everybody";
		int i=0;
//		indexOf tim chuoi dau tien tra ve, neu sai tra ve -1
		i=s.indexOf("e");
		System.out.println(i);
		
		
		int i1=0;
//		indexOf tim chuoi cuoi  tra ve, neu sai tra ve -1
		i1=s.lastIndexOf("e");
		System.out.println(i);
		System.out.println(i1);
	}
}
