package com.bai24;

public class BaiTapTachChuoi {

	public static String baiHatap3(String baiHat) {
		int vtCuoi =baiHat.lastIndexOf("/");
		String ten =baiHat.substring(vtCuoi+1);
		return ten;
	}
	public static void main(String[] args) {
		String baiHat = "D:/music/bolero/longme.mp3";
		String kq=baiHatap3(baiHat);
		System.out.println(kq);
	}
}
